  <body>
    <div id="navigation-bar">
      <nav class="navbar navbar-expand-lg navbar-dark">
        <a href="/">
          <img
            class="swingvasion-logo"
            href="/"
            src="images/swingvasion-logo.png"
            alt="Swingvasion-Logo"
          />
        </a>
        <button
          class="navbar-toggler"
          type="button"
          data-toggle="collapse"
          data-target="#navbarToggler"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarToggler">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item py-2 px-3">
              <a class="nav-link p-1 med-large" href="/">Home</a>
            </li>

            <li class="nav-item py-2 px-3">
              <a class="nav-link p-1 med-large" href="/tickets">Tickets</a>
            </li>

            <li class="nav-item py-2 px-3">
              <a class="nav-link p-1 med-large" href="/schedule">Schedule</a>
            </li>

            <li class="nav-item py-2 px-3 dropdown">
              <a
                class="nav-link p-1 med-large dropdown-toggle"
                id="eventInfoDropdown"
                data-toggle="dropdown"
                aria-haspopup="true"
                aria-expanded="false"
                >Event Info</a
              >
              <div
                class="dropdown-menu dropdown-menu-right py-0 px-4"
                aria-labelledby="eventInfoDropdown"
              >
                <a class="dropdown-item px-2 med-large" href="/staff">Staff</a>
                <a
                  class="dropdown-item last-dropdown-item px-2 med-large"
                  href="/competition-rules"
                  >Competition Rules</a
